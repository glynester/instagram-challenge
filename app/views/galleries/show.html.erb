<h1><%= @gallery.name %></h1>
<% if user_signed_in? %>
  <strong><%= link_to "Edit name", edit_gallery_path(@gallery) %></strong>
  <strong><%= link_to "Delete Gallery", gallery_path(@gallery), method: :delete %></strong>
<% end %>

<h2>Photos will display here.</h2>
<p><%= link_to "Back to Galleries listing", galleries_path %></p>
